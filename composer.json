        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/framework-bundle": "~2.8|~3.0",
        "victoire/victoire": "~1.5",
        "jms/serializer-bundle": "^1.0",
        "friendsofsymfony/elastica-bundle": "^3.1"
    },
    "autoload": {
        "psr-0": {
            "Victoire\\Widget\\SearchBundle": ""
